root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4


[*.java]
indent_size = 4

[{*.bash,*.sh,*.zsh}]
indent_size = 2
tab_width = 2

[{*.har,*.json,*.jsonc}]
indent_size = 2

[{*.markdown,*.md}]
indent_size = 4

[{*.yaml,*.yml}]
indent_size = 2

